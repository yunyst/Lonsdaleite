{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u-(?!load-more)(.*)$": "uview-ui/components/u-$1/u-$1.vue",
      "^u-load-more$": "uview-ui/components/u-loadmore/u-loadmore.vue",
      "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
    }
  },
