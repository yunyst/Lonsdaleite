{
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/home/home",
      "style": {
        "navigationBarTitleText": "Home"
      }
    },
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "index",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/404/404",
      "style": {
        "navigationBarTitleText": "404"
      }
    },
    {
      "path": "pages/account/account",
      "style": {
        "navigationBarTitleText": "Account",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/cart/cart",
      "style": {
        "navigationBarTitleText": "Cart",
        "enablePullDownRefresh": true
      }
    }

  ],
  "subPackages": [{
    "root": "subpages",
    "pages": [{
        "path": "news/news",
        "style": {
          "navigationBarTitleText": "News center",
          "enablePullDownRefresh": true
        }
      },
      {
        "path": "chat/chat",
        "style": {
          "navigationBarTitleText": "Online Chat"
        }
      }
    ]
  }],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "uniIdRouter": {},
  "tabBar": {
    "backgroundColor": "white",
    "color": "#ccc",
    "selectedColor": "#000000",
    "list": [{
        "pagePath": "pages/home/home",
        "iconPath": "/static/icons/loc.png",
        "selectedIconPath": "/static/icons/loc_fill.png",
        "text": "Home"
      },
      {
        "pagePath": "pages/cart/cart",
        "iconPath": "/static/icons/cart.png",
        "selectedIconPath": "/static/icons/cart_filled.png",
        "text": "Cart"
      },
      {
        "pagePath": "pages/account/account",
        "iconPath": "/static/icons/person.png",
        "selectedIconPath": "/static/icons/pesron_fill.png",
        "text": "Mine"
      }
    ]
  }
}